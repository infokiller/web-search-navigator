{
    "version": "1.0.0",
    "name": "web-search-navigator",
    "description": "Keyboard shortcuts for Google™ search.",
    "repository": "https://github.com/infokiller/web-search-navigator",
    "author": "https://github.com/infokiller",
    "contributors": [
